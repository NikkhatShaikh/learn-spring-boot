
spring.application.name=nikkhatSpringDemo

database configuration
spring.datasource.url=jdbc:mysql://localhost:3306/codekul?createDatabaseIfNotExist=true
spring.datasource.username=root
spring.datasource.password=root

Jpa Configuration
spring.jpa.hibernate.ddl-auto=update
spring.jpa.database-platform=org.hibernate.dialect.MySQL8Dialect

// security

add spring security dependency first
spring.security.user.name= nikkhat
spring.security.user.password=nikkhatshaikh123
